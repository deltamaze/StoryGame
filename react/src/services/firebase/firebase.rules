service cloud.firestore {
  match /databases/{database}/documents {
    match /Projects/{document=**} {
      allow read: if true;
      allow write: if false;
    }
    match /StoryGame/Usernames/{userId} {
      allow read: if true;
      allow write:if request.auth.uid == userId;
    }
    match /{document=**} {
      allow read: if false;
      allow write: if false;
    }
  }
}